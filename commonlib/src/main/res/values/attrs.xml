<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LabelEditText">
        <attr name="label_text" format="string" />
        <attr name="label_text_size" format="integer" />
        <attr name="label_text_color" format="color" />
        <attr name="label_padding" format="dimension" />
    </declare-styleable>


    <declare-styleable name="LabelTextView">
        <attr name="label_text_top" format="string" />
        <attr name="label_text_left" format="string" />
        <attr name="label_text_right" format="string" />
        <attr name="label_text_button" format="string" />
        <attr name="label_text_size" />
        <attr name="label_text_color" />
        <attr name="label_padding" />
    </declare-styleable>

    <declare-styleable name="TopBottomTextView">
        <attr name="label_text_top_str" format="string" />
        <attr name="label_text_bottom" format="string" />
        <attr name="label_text_top_size" format="integer" />
        <attr name="label_text_top_color" format="reference" />
        <attr name="label_text_bottom_size" format="integer" />
        <attr name="label_text_bottom_color" format="reference" />

        <attr name="label_TB_padding" format="integer" />
        <attr name="label_top_padding" format="integer" />
        <attr name="label_left_padding" format="integer" />
    </declare-styleable>


    <declare-styleable name="LoadFrameLayout">
        <attr name="emptyView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="loadingView" format="reference" />
    </declare-styleable>

</resources>